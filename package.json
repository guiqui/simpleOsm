{
  "name": "simpleosm",
  "version": "0.1.0",
  "description": "A simple component that can render openStreet Maps",
  "main": "openStreet.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --config webpack.config.demo.js ",
    "prod": "webpack --mode production --open --config webpack.config.prod.js ",
    "build": "webpack --mode production  --config webpack.config.build.js",
    "lint": "eslint src",
    "test": "jest && codecov",
    "prepublish": "rm -rf ./dist && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guiqui/simpleOsm.git"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.4"
  },
  "keywords": [
    "openstreet"
  ],
  "author": "Guillermo Quiros",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guiqui/simpleOsm/issues"
